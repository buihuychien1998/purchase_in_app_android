<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.store.journey">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_kindpng_2572965"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_kindpng_2572965"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.store.journey.SplashScreenActivity"
            android:theme="@style/SplashTheme"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.store.journey.MainActivity" />
        <activity
            android:name="com.store.journey.PlaceDetailActivity"
            android:parentActivityName="com.store.journey.PlaceDetailActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.store.journey.MainActivity" />

        </activity>
    </application>

</manifest>